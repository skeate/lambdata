{
  "name": "lambdata",
  "version": "1.0.0",
  "description": "Purely functional data structures in Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "keywords": [
    "pure",
    "functional",
    "data",
    "structures",
    "typescript",
    "fp",
    "programming",
    "okasaki"
  ],
  "author": "Jonathan Skeate",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/prettier": "1.19.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.18",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "fast-check": "^1.22.2",
    "fp-ts-laws": "^0.2.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "fp-ts": "^2.5.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "maxHeaderWidth": 60,
      "maxLineWidth": 72
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": ["./src/**/*.ts"]
  }
}
